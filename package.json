{
    "name": "syntax",
    "displayName": "sbt-rats Syntax",
    "description": "sbt-rats Syntax Language Support",
    "version": "0.0.0",
    "publisher": "inkytonik",
    "author": {
        "name": "Anthony M. Sloane",
        "email": "inkytonik@gmail.com",
        "url": "https://bitbucket.org/inkytonik/"
    },
    "license": "MPL-2.0",
    "homepage": "https://bitbucket.org/inkytonik/vscode-syntax",
    "bugs": "https://bitbucket.org/inkytonik/vscode-syntax/issues",
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/inkytonik/vscode-syntax"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "syntax",
            "aliases": ["Syntax", "syntax"],
            "extensions": [".syntax"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "syntax",
